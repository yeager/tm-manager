#!/usr/bin/env python3
import sys
import os
sys.path.insert(0, '/usr/lib/python3/dist-packages')

try:
    from tm_manager.main import main
    main()
except ImportError:
    try:
        from tm_manager.app import main
        main()
    except ImportError:
        print("Error: Cannot find main module for tm-manager", file=sys.stderr)
        sys.exit(1)
